      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@biomejs/biome": "1.3.3",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0"
